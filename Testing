public class Main {
    public static void main(String[] args) {
        System.out.println("=== Stack Test ===");
        MyStack stack = new MyStack();
        stack.push("A");
        stack.push("B");
        System.out.println(stack.pop()); // B
        System.out.println(stack.peek()); // A

        System.out.println("\n=== Queue Test ===");
        MyQueue queue = new MyQueue();
        queue.enqueue("X");
        queue.enqueue("Y");
        System.out.println(queue.dequeue()); // X
        System.out.println(queue.peek());    // Y

        System.out.println("\n=== MinHeap Test ===");
        MyMinHeap heap = new MyMinHeap();
        heap.add(5);
        heap.add(3);
        heap.add(7);
        System.out.println(heap.poll()); // 3
        System.out.println(heap.peek()); // 5
    }
}
